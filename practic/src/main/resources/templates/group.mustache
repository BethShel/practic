<html>
<body>
<button><form action="/" method="get">
    <input type="submit" value="Main menu"/>
</form></button>
<button><li><a href="/group">Back</a></li></button>
<div>
    <form method="post">
        <input type="text" name="name" placeholder="Add name"/>
        <input type="text" name="startdate" placeholder="Add startdate"/>
        <input type="text" name="enddate" placeholder="Add enddate"/>
        <input type="text" name="style" placeholder="Add style"/>
        <button type="submit">Add</button>
    </form>
</div>
<div>Group List</div>
<form method="post" action="groupFilter">
    <input type="text" name="filter">
    <button type="submit">Find</button>
</form>
{{#characteristics}}
    <div>
        <b>{{id}}</b>
        <span>{{name}}</span>
        <b>{{startdate}}</b>
        <span>{{enddate}}</span>
        <i>{{style}}</i>
    </div>
{{/characteristics}}
</body>
</html>