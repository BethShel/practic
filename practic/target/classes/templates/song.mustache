<html>
<body>
<button><form action="/" method="get">
    <input type="submit" value="Main menu"/>
</form></button>
<button><li><a href="/song">Back</a></li></button>
<div>
    <form method="post">
        <input type="text" name="name" placeholder="Add name"/>
        <input type="text" name="writedate" placeholder="Add writedate"/>
        <input type="text" name="style" placeholder="Add style"/>
        <input type="text" name="length" placeholder="Add length"/>
        <button type="submit">Add</button>
    </form>
</div>
<div>Songs List</div>
<form method="post" action="songFilter">
    <input type="text" name="filter">
    <button type="submit">Find</button>
</form>
{{#characteristics}}
    <div>
        <b>{{id}}</b>
        <span>{{name}}</span>
        <b>{{writedate}}</b>
        <i>{{style}}</i>
        <span>{{length}}</span>
    </div>
{{/characteristics}}
</body>
</html>